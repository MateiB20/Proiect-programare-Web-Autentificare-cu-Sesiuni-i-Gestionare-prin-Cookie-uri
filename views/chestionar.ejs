<header>
    <h1><img src="jordan-icon.ico" alt="mj" width="50" height="50">Magazin sneakers-Chestionar</h1>
</header>
<% if(reqAuth!==undefined){ %>
    <h1> Autentificat ca <%= reqAuth %>!</h1>

    <form method="POST" action="/delogare">
        <input type="submit" value="Log out">
        </form>
           <% } %>
<form method="POST" action="/rezultat-chestionar">
<% for (let i = 0; i < intrebari.length; i++) { %>
    <p><%= i + 1 %>. <%= intrebari[i].intrebare %></p>
    <% for (let j = 0; j < intrebari[i].variante.length; j++) { %>
            <input type="radio" name="<%= i %>" value="<%= j %>""><%= intrebari[i].variante[j] %><br>
        <% } %>
  <% } %>
<input type="submit" value="Submit">
</form>
<footer>
    <p>&copy; 2025 Magazin sneakers-Chestionar</p>
</footer>